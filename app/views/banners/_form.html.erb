<%= simple_form_for(@banner) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :images %>
    <%= f.input :mobile_key %>
    <%= f.input :desktop_key %>
    <%= f.input :desktop_size %>
    <%= f.input :mobile_size %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
