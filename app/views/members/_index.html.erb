<div class="well well-small">
  <%= form_for_filterrific @filterrific do |f| %>
    <div>
      Search:
      <%= f.text_field(:search_query, class: 'filterrific-periodically-observed') %>
      Search By Flat no:
      <%= f.text_field(:flat_no_search, class: 'filterrific-periodically-observed') %>
      Registered after:
      <%= f.text_field(:with_created_at_gte, class: 'js-datepicker') %>
    </div>
    <%= link_to('Reset filters', reset_filterrific_url) %>
    <%= render_filterrific_spinner %>
  <% end %>
</div>


<h1>Listing Members</h1>
<div id="filterrific_results"><%= render partial: 'members/list', locals: { members: @members } %></div>

<br>
